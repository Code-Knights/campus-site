{
  "name": "campus-site",
  "version": "0.1.1",
  "description": "campus-site",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "publish:": "pnpm commit && pnpm release:patch && git push --follow-tags origin main",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Code-Knights/campus-site.git"
  },
  "author": "miyucutee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Code-Knights/campus-site/issues"
  },
  "homepage": "https://github.com/Code-Knights/campus-site#readme",
  "dependencies": {
    "dotenv-cli": "^7.1.0",
    "next": "^13.2.*",
    "nextra": "latest",
    "nextra-theme-docs": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "standard-version": "^9.5.0",
    "swr": "^2.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@commitlint/cz-commitlint": "^17.4.4",
    "@release-it/conventional-changelog": "^5.1.1",
    "@types/node": "18.11.10",
    "commitizen": "^4.3.0",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "release-it": "^15.8.0",
    "typescript": "^4.9.3",
    "husky": "^8.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
